@import url('maplibre-gl/dist/maplibre-gl.css');
@import url('@mapbox/mapbox-gl-draw/dist/mapbox-gl-draw.css');

.custom-tooltip {
  @apply flex flex-col gap-1 z-100 prose p-0 m-0;


  & &__header,
  & &__body {
    @apply py-0 px-2;
  } 

  & &__header {
    @apply text-sm uppercase;
  }

  & &__body {
    @apply opacity-80;
  }
}
